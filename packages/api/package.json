{
  "name": "@marble/api",
  "version": "0.0.0",
  "description": "SDK library generated by Nestia",
  "private": true,
  "main": "lib/api/index.js",
  "typings": "lib/api/index.d.ts",
  "scripts": {
    "predev": "npm run build:sdk && npm run compile",
    "build:sdk": "rimraf ../../apps/backend/src/api/functional && cd ../../apps/backend && npx nestia sdk && cd ../../packages/api",
    "compile": "rimraf lib && tsc",
    "prepare": "ts-patch install && typia patch"
  },
  "files": [
    "lib",
    "package.json"
  ],
  "dependencies": {
    "@nestia/fetcher": "^3.0.0",
    "typia": "^5.3.4"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.1",
    "typescript": "5.3.2"
  }
}
